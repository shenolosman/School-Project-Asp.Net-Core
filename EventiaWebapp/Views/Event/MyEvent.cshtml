@{
    ViewBag.Title = "Event | My Events";
    int counter = 1;
}
<div class="row">
    <p>
        @*
            _eventHandler.Attendees.Find(attendee =>attendee.AttendeeName.Contains("Shenol"));
        *@
    </p>
</div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Event Date</th>
            <th scope="col">Event Location</th>
            <th scope="col">Event Name</th>
            <th scope="col">Organizer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in _eventHandler.MyEvents)
        {
            <tr>
                <th scope="row">@counter</th>
                <td>@item.EventDate</td>
                <td> @item.EventLocation</td>
                <td>@item.EventName</td>
                <td>@item.Organizer.OrganizerName</td>
            </tr>
            counter++;
        }
    </tbody>
</table>